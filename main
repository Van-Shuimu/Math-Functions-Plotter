import numpy as np
import matplotlib.pyplot as plt
import math as m


def fun(x):
    return -1/(3*x)
def fun1(x):
    return None

def fun2(x):
    return None

xl = 5
zero = 1
st = 6
xlm = 2

ypl = []
xall = np.linspace(-xl,xl,xl*xlm**st+zero)
for x in xall:
    #ypl.append(x)
    ypl.append((fun(x),fun1(x),fun2(x)))
plt.plot(xall,ypl,'-')
plt.grid('True')
plt.axis('Equal')
plt.show()




